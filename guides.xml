<?xml version="1.0" encoding="UTF-8" ?>
<guides
    xmlns="https://www.phpdoc.org/guides"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://www.phpdoc.org/guides vendor/phpdocumentor/guides-cli/resources/schema/guides.xsd"
    links-are-relative="true"
>
    <extensions>
        <bootstrap>
            <class>\phpDocumentor\Guides\Bootstrap\DependencyInjection\BootstrapExtension</class>
        </bootstrap>
        <graphs>
            <class>\phpDocumentor\Guides\Graphs\DependencyInjection\GraphsExtension</class>
        </graphs>
        <markdown>
            <class>\phpDocumentor\Guides\Markdown\DependencyInjection\MarkdownExtension</class>
        </markdown>
        <typo3theme>
            <class>\T3Docs\Typo3DocsTheme\DependencyInjection\Typo3DocsThemeExtension</class>
        </typo3theme>
        <typo3docs>
            <class>\T3Docs\GuidesExtension\DependencyInjection\Typo3GuidesExtension</class>
        </typo3docs>
        <phpdomain>
            <class>\T3Docs\PhpDomain\DependencyInjection\PhpDomainExtension</class>
        </phpdomain>
    </extensions>
    <inventories>
        <inventory id="h2document" url="https://docs.typo3.org/m/typo3/docs-how-to-document/main/en-us/" />
    </inventories>
    <theme>typo3docs</theme>
    <output-format>html</output-format>
    <output-format>singlepage</output-format>
    <output-format>interlink</output-format>
</guides>
