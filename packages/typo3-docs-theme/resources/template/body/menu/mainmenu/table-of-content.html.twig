<nav>
    <input class="toc-checkbox" id="toggleToc" type="checkbox"/>
    <div class="toc-header">
        <div class="toc-actions">
            <label class="toc-toggle" for="toggleToc">
                Menu
            </label>
        </div>
    </div>
    <div class="toc-collapse">
        <div aria-label="main navigation" class="toc" role="navigation">
            <!-- menu -->
            <ul>
    {% for entry in node.entries -%}
        <li class="toctree-l1"><a class="reference external" href="{{ renderLink(entry.url) }}">{{ renderNode(entry.value.value) }}</a></li>

        {%- if entry.children|length %}
            {% include "body/menu/mainmenu/menu-level.html.twig" with {
                    tocItems:tocItem.children
            } %}
        {%- endif -%}
    {% endfor %}
            </ul>
        </div>
    </div>
</nav>
