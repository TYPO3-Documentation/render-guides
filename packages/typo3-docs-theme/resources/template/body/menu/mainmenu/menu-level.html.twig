{% apply spaceless %}
    {% for entry in node.entries %}
        <li class="toctree-l2">
            <a class="reference internal" href="{{ renderLink(entry.url) }}" {% if entry.options.active %}  aria-current="page" {% endif %}>
                {{ renderNode(entry.value.value) }}
            </a>
        </li>
    {% endfor %}
{% endapply %}
