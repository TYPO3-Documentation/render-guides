<li class="toctree-l{{ menuEntry.level }} {%- if menuEntry.isInRootline %} current {% endif %}">
    <a href="{{ renderLink(menuEntry.url) }}"
       class="reference internal">
        {{ renderNode(menuEntry.value.value) }}
    </a>

    {%- if menuEntry.children|length %}
        <ul>
            {%- for entry in menuEntry.children -%}
                {% include "body/menu/sitemap-level.html.twig" with {
                    menuEntry:entry
                } %}
            {%- endfor -%}
        </ul>
    {%- endif -%}
</li>
