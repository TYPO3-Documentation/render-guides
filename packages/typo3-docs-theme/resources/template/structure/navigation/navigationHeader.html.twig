
<div class="toc-header">
    <div class="toc-title pe-3">
        {% set topPageLink = getTopPageLink() %}
        {% if topPageLink %}
            <a class="toc-title-project" href="{% if has_page_context %}{{ topPageLink.url }}{% else %}../Index.html{% endif %}">{{ topPageLink.title }}</a>
        {% endif %}
    </div>
    {% if env.projectNode.version and not getSettings('disable_version_switch') -%}
        <label for="languageSelect" class="visually-hidden">Select language</label>
        <select id="languageSelect" class="form-select d-none">
        </select>

        {% set proxyAttributes = '' %}
        {% if not isRenderedForDeployment() %}
          {% set proxyAttributes = 'data-override-url-proxy="' ~ getRelativePath('_resources/js/versions-proxy.php') ~ '?url=" data-override-url-self="https://docs.typo3.org/m/typo3/tutorial-getting-started/12.4/en-us/Concepts/Index.html"' %}
        {% endif %}

        <label for="versionSelect" class="visually-hidden">Select version</label>
        <select id="versionSelect" class="form-select"
                data-current-version="{{ env.projectNode.version }}"
            {{ proxyAttributes|raw }}
        >
            <option disabled selected>Loading versions...</option>
        </select>
    {% endif %}
</div>
