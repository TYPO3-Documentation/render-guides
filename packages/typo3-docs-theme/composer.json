{
    "name": "t3docs/theme",
    "description": "Theme for TYPO3 Documentation",
    "license": "MIT",
    "keywords": [
        "rst",
        "restructuredtext"
    ],
    "homepage": "https://github.com/TYPO3-Documentation/typo3-docs-theme",
    "require": {
        "php": "^8.0",
        "doctrine/rst-parser": "^0.6@dev",
        "t3docs/intersphinx": "dev-main"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.4.1",
        "doctrine/coding-standard": "^10.0",
        "ergebnis/composer-normalize": "2.28.*",
        "gajus/dindent": "^2.0.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^9.5",
        "symfony/css-selector": "^6.0",
        "symfony/dom-crawler": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "T3Docs\\Theme\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T3Docs\\Tests\\Theme\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    }
}
