{
    "name": "t3docs/typo3-docs-theme",
    "description": "A theme for use in the phpdocumentor",
    "type": "library",
    "license": "MIT",
    "homepage": "https://docs.typo3.org",
    "autoload": {
        "psr-4": {
            "T3Docs\\Typo3DocsTheme\\": "src/"
        }
    },
    "require": {
        "brotkrueml/twig-codehighlight": "dev-main",
        "phpdocumentor/guides-graphs": "dev-main",
        "phpdocumentor/guides-theme-bootstrap": "dev-main",
        "t3docs/typo3-version-handling": "self.version"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brotkrueml/twig-codehighlight"
        }
    ]
}
